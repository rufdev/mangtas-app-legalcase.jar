import com.rameses.annotations.*;

class HousingLedgerEntryService {
	
	@DataContext("housing_ledger_payment_item")
	def hle;

	@ProxyMethod
	public def getList( def o ) {
		if(!o.ledgerid) return [];
		return hle.find( ['parent.ledgerid': o.ledgerid ] ).select("objid,parent.receiptno,parent.receiptdate,year,month,amount").list();
	}


}